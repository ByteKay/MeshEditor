material Robot_F_INST
{
	technique 0
	{
		pass 0
		{
			vertexShader = res/shaders/textured.vert
			fragmentShader = res/shaders/textured.frag
			defines = BUMPED
			u_worldViewProjectionMatrix = WORLD_VIEW_PROJECTION_MATRIX
			sampler u_diffuseTexture
			{
				path = res/roles/test/F_D.png
				wrapS = REPEAT
				wrapT = REPEAT
				mipmap = true
				minFilter = NEAREST_MIPMAP_LINEAR
				magFilter = LINEAR
			}
			sampler u_normalmapTexture
			{
				path = res/roles/test/F_N.png
				wrapS = REPEAT
				wrapT = REPEAT
				mipmap = true
				minFilter = NEAREST_MIPMAP_LINEAR
				magFilter = LINEAR
			}
			renderState 
			{
				depthTest = true
				cullFace = false
			}
		}
	}
}
